{% endif %}

# tell registrar, which url prefixes has custom wayf and should be used for identity consolidation
# if not set, user is redirected to /fed/ for federated identities (registrar->ic)
#registrar.customWayfsPrefixes=elixir,bbmri

{% if perun_oldgui_registrar_skipSummaryFor is defined %}
# List of Vos and groups for which we skip summary page if no error happens and we redirect outside of registrar afterwards
# format: "vo_short_name" or "vo_short_name:group_name".
registrar.skipSummaryFor={{ perun_oldgui_registrar_skipSummaryFor|join(', ') }}
{% endif %}

# When set to true, finding of similar users after clicking on the registration submit button will not be triggered.
# Therefore, account linking will not be offered to users even when they already have registered some similar accounts.
registrar.findSimilarUsers.disabled={{ perun_oldgui_registrar_findSimilarUsers_disabled }}

# List of proxies from which we hide identities during VO/Group registration.
registrar.hideProxy=

### User Profile ###

# defines urn of user attributes that should be displayed on personal page
# you can force showing custom name and/or description like [name]urn[description]
# or you can have translated like: [English|NativeName]urn:perun:user:attribute-def:core:displayName[English|NativeDescription]
profile.personal.showAttributes={{ perun_oldgui_profile_personal_showAttributes|join(', ') }}

### {pages to hide: 'identities', 'organizations', 'groups', 'resources', 'privacy'}
